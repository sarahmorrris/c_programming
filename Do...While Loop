#include <stdio.h>

int main() {
    int count = 10;
    int number;
    printf("Enter a number: ");
    scanf("%d", &number);
   
   do{
       int product = number * count;
       printf("%d*%d = %d\n", number, count, product);
       count = count - 1;
       
   } while ((count <= 10) && (count > 0));
    
    return 0;
}
